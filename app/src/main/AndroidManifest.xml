<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.myeco.sayour">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sayour"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.artikel.DetailArtikel"></activity>
        <activity android:name=".ui.Profil">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ui.katalog.DetailKatalog" />
        </activity>
        <activity android:name=".ui.artikel.Artikel" />
        <activity android:name=".ui.katalog.DetailKatalog" />
        <activity android:name=".ui.katalog.Katalog" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>